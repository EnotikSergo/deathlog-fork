  ],
  "contact": {
    "issues": "https://github.com/EnotikSergo/deathlog-fork/issues",
    "sources": "https://github.com/EnotikSergo/deathlog-fork"
  },
  "license": "MIT",
  "icon": "assets/deathlog/icon.png",
  "environment": "*",
